[
	{
		"name": "comicextra",
		"lang": "en",
		"type": "hq",
		"priority": 2,
		"color": "ffffcf40",
		"search": {
			"request": {
				"url": "https://comicextra.me/search?keyword={query}&page={page}",
				"type": "get",
				"pagination": "true"
			},
			"rows": "find_all('div', ('class','cartoon-box'))",	
			"title": "item('a', order=2)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "'\\n'.join(x.strip() for x in item('div', order=4).split('\\n'))",
			"link": "item('a', attribute='href', order=2)"
		},
		"popular": {
			"request": {
				"url": "https://comicextra.me/popular-comic/{page}",
				"type": "get",
				"pagination": "true"
			},
			"rows": "find_all('div', ('class','cartoon-box'))",	
			"title": "item('a', order=2)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "item('div', order=4)",
			"link": "item('a', attribute='href', order=2)"
		},
		"chapters": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('table', ('class', 'table')).find_all('tr')",
			"title": "item('a', order=1)",
			"plot": "'\\n'.join(x.strip() for x in item('td', order=2).split('\\n'))",
			"link": "item('a', attribute='href', order=1) + '/full'",
			"reverse": "true"
		},
		"pages": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('div', ('class', 'chapter-container')).find_all('img')",
			"title": "item.attr['alt']",
			"link": "item.attr['src']",
			"reverse": "true"
		}
	},



	{
		"name": "mangaonline",
		"lang": "pt-BR",
		"type": "manga",
		"priority": 0,
		"color": "ff8b9dc3",
		"search": {
			"request": {
				"url": "https://mangaonline.biz/search/{query}",
				"type": "get"
			},
			"rows": "find_all('div', ('id','archive-content'))",	
			"title": "item('a', order=2)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "item('div', order=2)",
			"link": "item('a', attribute='href', order=2)"
		},
		"popular": {
			"request": {
				"url": "https://mangaonline.biz/mais-vistos/",
				"type": "get"
			},
			"rows": "find_all('div', ('id','archive-content'))",
			"title": "item('a', order=2)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "item('div', order=2)",
			"link": "'https://mangaonline.biz' + item('a', attribute='href', order=2)"
		},
		"chapters": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('ul', ('class', 'episodios')).find_all('li')",
			"title": "item('a', order=1)",
			"plot": "item('a', order=1).split('/')[-1]",
			"link": "item('a', attribute='href', order=1)",
			"reverse": "true"
		},
		"pages": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('div', ('class', 'content')).find_all('img', ('loading', 'lazy'))",
			"title": "item.attr['src']",
			"mutate_title": "'{title}'.split('/')[-1]",
			"link": "item.attr['src']",
			"reverse": "false"
		}
	},



	{
		"name": "mangananquim",
		"lang": "pt-BR",
		"type": "manga",
		"priority": 0,
		"color": "ffbe9b7b",
		"search": {
			"request": {
				"url": "https://mangananquim.com/?s={query}&post_type=wp-manga",
				"type": "get"
			},
			"rows": "find_once('div', ('class','c-tabs-item')).find_all('div', ('class','c-tabs-item__content'))",
			"title": "item('a', attribute='title', order=1)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "",
			"link": "item('a', attribute='href', order=1) + 'ajax/chapters/'"
		},
		"popular": {
			"request": {
				"url": "https://mangananquim.com/page/{page}/?m_orderby=trending",
				"type": "get",
				"pagination": "true"
			},
			"rows": "find_once('div', ('role','tabpanel')).find_all('div', ('class','page-item-detail'))",
			"title": "item('a', attribute='title', order=1)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "item('a', select=('class','btn-link'), order=1)",
			"link": "item('a', attribute='href', order=1) + 'ajax/chapters/'"
		},
		"chapters": {
			"request": {
				"type": "post"
			},
			"rows": "find_all('li', ('class','wp-manga-chapter'))",
			"title": "item('a', order=1)",
			"plot": "'adicionado em ' + item('i', order=1)",
			"link": "item('a', attribute='href', order=1) + '?style=list'",
			"reverse": "true"
		},
		"pages": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('div', ('class', 'reading-content')).find_all('img', ('class', 'wp-manga-chapter-img'))",
			"title": "item.attr['id']",
			"link": "item.attr['src']",
			"reverse": "false"
		}
	},



	{
		"name": "galaxiadosquadrinhos",
		"lang": "pt-BR",
		"type": "hq",
		"priority": 0,
		"color": "ff36802d",

		"search": {
			"request": {
				"url": "https://www.galaxiadosquadrinhos.com.br/feeds/posts/default?alt=json&max-results=200&q={query}&start-index={page}",
				"type": "get",
				"pagination": "true",
				"page_multiplier": "int(page)*200 - 199"
			},
			"type": "json",
			"exclude_title_with_words": "#|Ofertas - |Marvel Comics|POP Funko",
			"rows": "['feed']['entry']",
			"title": "['title']['$t']",
			"image": "['media$thumbnail']['url']",
			"plot": "",
			"link": "['link']",
			"mutate_link": "list(filter(lambda x: True if x['type'] == 'text/html' and x['rel']=='alternate' else False, {link}))[0]['href']"
		},
		"popular": {
			"request": {
				"url": "https://www.galaxiadosquadrinhos.com.br/feeds/posts/default?alt=json&max-results=200&start-index={page}",
				"type": "get",
				"pagination": "true",
				"page_multiplier": "int(page)*200 - 199"
			},
			"type": "json",
			"exclude_title_with_words": "#|Ofertas - |Marvel Comics|POP Funko",
			"rows": "['feed']['entry']",
			"title": "['title']['$t']",
			"image": "['media$thumbnail']['url']",
			"plot": "",
			"link": "['link']",
			"mutate_link": "list(filter(lambda x: True if x['type'] == 'text/html' and x['rel']=='alternate' else False, {link}))[0]['href']"
		},
		"chapters": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('div', ('class', 'post-body')).find_all('a', ('target','_blank'))",
			"title": "item('img', attribute='title', order=1)",
			"image": "item('img', attribute='src', order=1)",
			"plot": "",
			"link": "item.attr['href']",
			"reverse": "false"
		},
		"pages": {
			"request": {
				"type": "get"
			},
			"rows": "find_once('div', ('class', 'post')).find_all('img')",
			"title": "item.attr['src']",
			"mutate_title": "'{title}'.split('/')[-1]",
			"link": "item.attr['src']",
			"mutate_link": "'/'.join(['https:'] + '{link}'.replace('http:/', '').replace('https:/', '').split('/'))",
			"reverse": "false"
		}
	},



