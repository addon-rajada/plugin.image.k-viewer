	{
		"name": "comicextra",
		"lang": "en",
		"type": "hq",
		"priority": 2,
		"color": "ffffcf40",
		"search": {
			"request": {
				"url": "https://comicextra.me/search?keyword={query}&page={page}",
				"type": "get",
